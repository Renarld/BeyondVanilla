      {
         "when":{
            "south":"true"
         },
         "apply":{
            "model":"beyondvanilla:block/gold_fence_side",
            "y":180,
            "uvlock":false
         }
      },
      {
         "when":{
            "west":"true"
         },
         "apply":{
            "model":"beyondvanilla:block/gold_fence_side",
            "y":270,
            "uvlock":false
         }
      },
      {
         "when":{
            "up":"true"
         },
         "apply":{
            "model":"beyondvanilla:block/gold_fence_post_full"
