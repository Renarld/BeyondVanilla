{
   "multipart":[
      {
         "apply":{
            "model":"beyondvanilla:block/gold_fence_post"
         }
      },
      {
         "when":{
            "north":"true"
         },
         "apply":{
            "model":"beyondvanilla:block/gold_fence_side",
            "uvlock":false
         }
      },
      {
         "when":{
            "east":"true"
         },
         "apply":{
            "model":"beyondvanilla:block/gold_fence_side",
